{
  "name": "react-typescript-sass-webpack-starter",
  "title": "React Typescript SASS Webpack Starter",
  "version": "0.1.0",
  "author": "",
  "private": true,
  "description": "",
  "main": "",
  "license": "MIT",
  "engines": {
    "node": ">=16"
  },
  "config": {
    "port": "7700"
  },
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watchAll",
    "setup": "npm install && npm run message:setup && node tools/setup.js",
    "start": "webpack serve --history-api-fallback --config webpack.config.dev.js",
    "build": "webpack --progress --config webpack.config.build.js && npm run build:complete --silent",
    "build:analyze": "webpack --progress --config webpack.config.build.js --env=analyze && npm run build:complete --silent",
    "build:complete": "npm run prettier:html && npm run message:build-complete && npm run message:build-server",
    "prettier": "prettier --write \"src/**/*.{ts,tsx,css,scss,js,jsx,json}\"",
    "prettier:html": "prettier --write \"build/**/*.html\"",
    "lint": "eslint --fix src/**/*.{js,jsx,ts,tsx}",
    "lint:js": "eslint --fix src/**/*.{js,jsx}",
    "lint:ts": "eslint --fix src/**/*.{ts,tsx}",
    "message:setup": "node tools/messages/setup.js",
    "message:build-complete": "node tools/messages/build.js",
    "message:build-server": "node tools/server.js",
    "message:ip": "node tools/messages/ip.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@storybook/react": "^6.4.7",
    "@types/react": "^17.0.37",
    "animate-scss": "^3.2.1",
    "axios": "^0.24.0",
    "classnames": "^2.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-intersection-observer": "^8.32.5",
    "react-router-dom": "^6.0.2",
    "react-transition-group": "^4.4.2",
    "resolve-url-loader": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-typescript": "^7.16.0",
    "@storybook/addon-actions": "^6.4.7",
    "@storybook/addon-docs": "^6.4.7",
    "@storybook/addon-essentials": "^6.4.7",
    "@storybook/addon-links": "^6.4.7",
    "@storybook/builder-webpack5": "^6.4.7",
    "@storybook/manager-webpack5": "^6.4.7",
    "@svgr/webpack": "^6.1.1",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.11",
    "@types/react-transition-group": "^4.4.4",
    "animate-sass": "^0.8.2",
    "babel-loader": "^8.2.3",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-es2015": "^6.24.1",
    "breakpoint-sass": "^2.7.1",
    "chalk": "^4.1.2",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^10.0.0",
    "css-loader": "^6.5.1",
    "ejs-loader": "^0.5.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb-typescript-prettier": "^4.2.0",
    "eslint-webpack-plugin": "^3.1.1",
    "fibers": "^5.0.0",
    "file-loader": "^6.2.0",
    "html-loader": "^3.0.1",
    "html-webpack-inline-source-plugin": "0.0.10",
    "html-webpack-plugin": "^5.5.0",
    "ip": "^1.1.5",
    "jest": "^27.4.3",
    "mini-css-extract-plugin": "^2.4.5",
    "postcss": "^8.4.4",
    "postcss-browser-reporter": "^0.6.0",
    "postcss-discard-comments": "^5.0.1",
    "postcss-flexibility": "^3.0.0",
    "postcss-import": "^14.0.2",
    "postcss-loader": "^6.2.1",
    "postcss-preset-env": "^7.0.1",
    "postcss-reporter": "^7.0.4",
    "postcss-sass-loader": "^1.1.0",
    "postcss-scss": "^4.0.2",
    "postcss-url": "^10.1.3",
    "prettier": "^2.5.1",
    "prompt": "^1.2.0",
    "raw-loader": "^4.0.2",
    "replace": "^1.2.1",
    "rimraf": "^3.0.2",
    "sass": "^1.44.0",
    "sass-loader": "^12.3.0",
    "source-map-loader": "^3.0.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.2",
    "webpack": "^5.64.4",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.6.0",
    "webpack-merge": "^5.8.0"
  }
}
