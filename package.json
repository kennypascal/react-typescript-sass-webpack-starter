{
  "name": "react-typescript-sass-webpack-starter",
  "title": "React Typescript SASS Webpack Starter",
  "version": "0.1.0",
  "author": "",
  "private": true,
  "description": "",
  "main": "",
  "license": "MIT",
  "config": {
    "port": "7700",
    "port-storybook": "7800"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "setup": "npm install && npm run message:setup && node tools/setup.js",
    "start": "npm run message:ip && webpack-dev-server -d --history-api-fallback --hot --inline --progress --colors --config webpack.config.dev.js",
    "build": "webpack -p --progress --colors --config webpack.config.build.js && npm run build:complete --silent",
    "build:analyze": "webpack -p --progress --colors --config webpack.config.build.js --env.analyze && npm run build:complete --silent",
    "build:complete": "npm run prettier:html && npm run message:build-complete && npm run message:build-server",
    "prettier": "prettier --write \"src/**/*.{ts,tsx,css,scss,js,jsx,json}\"",
    "prettier:html": "prettier --write \"build/**/*.html\"",
    "lint": "eslint --fix src/**/*.{js,jsx,ts,tsx}",
    "lint:js": "eslint --fix src/**/*.{js,jsx}",
    "lint:ts": "eslint --fix src/**/*.{ts,tsx}",
    "storybook": "start-storybook -s ./src/assets --port $npm_package_config_port_storybook",
    "storybook:build": "build-storybook -s assets",
    "message:setup": "node tools/messages/setup.js",
    "message:build-complete": "node tools/messages/build.js",
    "message:build-server": "node tools/server.js",
    "message:ip": "node tools/messages/ip.js"
  },
  "dependencies": {
    "@storybook/addon-knobs": "^6.0.21",
    "@storybook/react": "^6.0.21",
    "@types/react": "^16.9.49",
    "@types/storybook__react": "^5.2.1",
    "animate-scss": "^3.2.1",
    "classnames": "^2.2.6",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hot-loader": "4.12.21",
    "react-router-dom": "^5.2.0",
    "react-transition-group": "^4.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@hot-loader/react-dom": "^16.13.0",
    "@types/node": "^14.11.1",
    "@types/react-transition-group": "^4.4.0",
    "animate-sass": "^0.8.2",
    "babel-loader": "^8.1.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-es2015": "^6.24.1",
    "breakpoint-sass": "^2.7.1",
    "chalk": "^4.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.1.1",
    "css-loader": "^4.3.0",
    "ejs-loader": "^0.5.0",
    "eslint": "^7.9.0",
    "eslint-config-airbnb-typescript-prettier": "^3.1.0",
    "fibers": "^5.0.0",
    "file-loader": "^6.1.0",
    "html-loader": "^1.3.0",
    "html-webpack-inline-source-plugin": "0.0.10",
    "html-webpack-plugin": "^4.4.1",
    "ip": "^1.1.5",
    "mini-css-extract-plugin": "^0.11.2",
    "node-sass": "^4.14.1",
    "postcss": "^8.0.5",
    "postcss-browser-reporter": "^0.6.0",
    "postcss-discard-comments": "^4.0.2",
    "postcss-flexibility": "^2.0.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^4.0.2",
    "postcss-preset-env": "^6.7.0",
    "postcss-reporter": "^7.0.0",
    "postcss-sass-loader": "^1.1.0",
    "postcss-scss": "^3.0.0",
    "postcss-url": "^8.0.0",
    "prettier": "^2.1.2",
    "prompt": "^1.0.0",
    "raw-loader": "^4.0.1",
    "replace": "^1.2.0",
    "rimraf": "^3.0.2",
    "sass": "^1.26.11",
    "sass-loader": "^10.0.2",
    "source-map-loader": "^1.1.0",
    "style-loader": "^1.2.1",
    "terser-webpack-plugin": "^4.2.1",
    "ts-loader": "^8.0.3",
    "typescript": "^4.0.3",
    "webpack": "^4.44.2",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.1.4"
  }
}
